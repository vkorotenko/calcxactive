  "description": "Офлайн калькулятор расчета огнезащитного покрытия",
  "license": "MIT",
  "author": {
    "name": "Vladimir N. Korotenko",
    "email": "koroten@ya.ru",
    "url": "http://vkorotenko.ru"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vkorotenko/calcxactive"
  },
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
