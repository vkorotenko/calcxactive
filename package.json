    "html-webpack-inline-source-plugin": "*",
    "html-webpack-plugin": "*",
    "serialize-javascript": "*",
    "vue-template-compiler": "*"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
